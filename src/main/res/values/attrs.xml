<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialButton">
        <attr name="text" format="string"/>
        <attr name="textSize" format="float"/>
        <attr name="textColor" format="color"/>
        <attr name="eventClick" format="boolean"/>
        <attr name="effectColor" format="color"/>
        <attr name="effectPosition" format="boolean"/>
        <attr name="drawableCenter" format="integer"/>
        <attr name="drawableLeft" format="integer"/>
        <attr name="drawableRight" format="integer"/>
        <attr name="drawableTop" format="integer"/>
        <attr name="drawableBottom" format="integer"/>
        <attr name="drawablePadding" format="integer"/>
    </declare-styleable>
</resources>